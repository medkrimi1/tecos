    {% else %}

<div class="row min-vh-100 bg-200">
          
          <div class="col-sm-10 col-md-6 px-sm-0 align-self-center mx-auto py-5">
            <div class="row justify-content-center g-0">
              <div class="col-lg-9 col-xl-8 col-xxl-6">
                <div class="card">
                  <div class="card-header bg-500 m bg-shape text-center  p-3"><img src="{{ asset('images/logo-white.png')}}"  width="180"  alt=""></div>
                  <div class="card-body">
                   
              <form  method="POST" >  
                                    
                                    <h1 class=" popup-title text-center text-dark">Se connecter</h1> 
                                <p class="text-center">Si vous avez un compte chez nous, veuillez vous identifier</p>
                                 <div id="error"></div>

                                    <div class="form-group"> 
                                <div class="fs-0">Adresse e-mail</div>
                                 <input type="email" value="{{ last_username }}" name="email" id="inputEmail" class="form-control" autocomplete="email" required autofocus>
   
                                </div>
                                 <div class="form-group"> 
                                <div class="fs-0">Mot de passe</div>
                                 <input type="password" name="password" id="inputPassword" class="form-control" autocomplete="current-password" required>
 </div>
     <input type="hidden" name="_csrf_token"
           value="{{ csrf_token('authenticate') }}"
    >
   <div class="form-check">Remember Me<input class="form-check-input" id="flexCheckIndeterminate" type="checkbox" /><label class="form-check-label" for="flexCheckIndeterminate" name="_remember_me"></label></div>
         {% if error %}
        <div class="text-danger text-center m-b10">E-mail ou Mot de passe incorrect</div>
    {% endif %}
                                <div class="text-center">
                                
                                    <button type="submit" " class="btn btn-primary rounded-pill">Se connecter
                                    </button>
                                    <ul></ul>
                                    <div class="text-center" >
                                    <a data-bs-toggle="tab" href="#forgot-password" class=""><i class="fa fa-unlock-alt"></i> Mot de passe oublié ?</a>
                                </div>
                         
                                
                            </form>
                 
              
                  </div>
                </div>
              </div>
